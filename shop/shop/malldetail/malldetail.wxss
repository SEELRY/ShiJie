/* 收藏 */
.collectionIco{width: 80rpx;height: 80rpx;border-radius: 50%;text-align: center;line-height: 80rpx;background: #fff;border: 1px solid #e9e9e9;position: absolute;top: 20rpx;right: 20rpx;z-index: 1;font-size: 40rpx;color: #999;}
