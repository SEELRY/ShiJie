/* 领劵 */
.couponMiniList{display: inline-block;padding: 0 20rpx;line-height: 44rpx;background: #f15451;color: #fff;font-size: 24rpx;border-radius: 4rpx;position: relative;}
.couponMiniList:not(:last-child){margin-right: 16rpx;}
.couponMiniList::before,.couponMiniList::after{content: '';display: block;width: 12rpx;height: 12rpx;border-radius: 50%;background: #fff;position: absolute;top: 50%;transform: translateY(-50%);}
.couponMiniList::before{left: -6rpx;}
.couponMiniList::after{right: -6rpx;}
.couponPopList{background: #ffe8dc;color: #f15350;border-radius: 6rpx;overflow: hidden;position: relative;}
.couponPopList:not(:last-child){margin-bottom: 20rpx;}
.couponPopInfo{padding: 20rpx 40rpx;align-self: center;}
.couponListPrice{font-size: 28rpx;font-weight: bold;}
.couponListPriceNum{font-size: 40rpx;}
.couponListTips{font-size: 24rpx;line-height: 1.4;}
.couponListBtn{align-self: center;width:172rpx;text-align:center;font-size: 30rpx;line-height: 130rpx;padding-left: 40rpx;padding-right: 40rpx;border-left: 1px dashed;position: relative;z-index: 1;}
.couponListBtn::before,.couponListBtn::after{content: '';display: block;width: 18rpx;height: 18rpx;border-radius: 50%;background: #fff;position: absolute;left: -9rpx;}
.couponListBtn::before{top: -9rpx;}
.couponListBtn::after{bottom: -9rpx;}
.couponListIco{position: absolute;right: -30rpx;bottom: -40rpx;font-size: 140rpx;line-height: 1;color: #ffd6cb;opacity: .85;}

.couponPopList.receive{background: #eee;color: #999;}

/* 福利 */
.welfareInfoBox{padding: 16rpx 30rpx 16rpx;}
