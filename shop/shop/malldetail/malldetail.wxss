.cuxiao{display: inline-block; margin-right: 10rpx; height: 32rpx;width:32rpx; line-height: 32rpx;background: #fea512; color: #fff; border-radius: 3px; font-size: 12px;text-align: center;vertical-align:middle;}



/* 优惠券 */
.youhuiItem{display: inline-block;font-size: 24rpx;line-height: 36rpx;padding: 0 10rpx;background: #f15350;color: #fff;border-radius: 4rpx;margin-right: 10rpx;vertical-align: middle;max-width: 100%;}
.couponItemBox{padding: 20rpx;white-space: nowrap;overflow-x: auto;-webkit-overflow-scrolling: touch;background: #fff;}
