.welfareList{line-height: 1.4;padding: 10rpx 0;margin-bottom: 6rpx;}
.welfareListTag{display: inline-block;font-size: 24rpx;color: #f15451;padding: 0 8rpx;line-height: 32rpx;border-radius: 4rpx;position: relative;margin-right: 10rpx;align-self: flex-start;margin-top: 4rpx;}
.welfareListTag::before{content: '';display: block;width: 200%;height: 200%;border: 1px solid;box-sizing: border-box;position: absolute;top: 0;left: 0;transform: scale(.5);-webkit-transform-origin: 0 0;transform-origin: 0 0;pointer-events: none;border-radius: 8rpx;}
.becomeVipBtn{font-size: 24rpx;color: #f15451;margin-left: 10rpx;}



