.couponItem:not(:last-child){margin-right: 20rpx;}
.couponItemInfo{line-height: 1.3;border-right: 1px dashed rgba(255, 255, 255, 0.5);padding-right: 20rpx;position: relative;max-width: 200rpx;}
.couponItemInfo::before,.couponItemInfo::after{content: '';display: block;width: 16rpx;height: 16rpx;border-radius: 50%;background: #fff;position: absolute;right: -8rpx;}
.couponItemInfo::before{top:-18rpx;}
.couponItemInfo::after{bottom:-18rpx;}
.couponItemPriceNum{font-size: 36rpx;}
.couponItemBtn{background: transparent;font-size: 20rpx;width: 60rpx;white-space: pre-wrap;color: inherit;line-height: 1.4;padding: 0 20rpx;height: 82rpx;display: flex;align-items: center;justify-content: center;}
.couponReceived{background: #d3d3d3;}
/* 促销 */
.promotionItem{font-size: 28rpx;align-items: center;}
.promotionItemTitle{padding: 0 10rpx;line-height: 26rpx;display: inline-block;font-size: 20rpx;color: #f15350;border: 1px solid;border-radius: 28rpx;margin-right: 20rpx;}
.openMemberBtn{font-size: 20rpx;color: #f15350;border: 1px solid;padding: 0 10rpx;line-height: 26rpx;border-radius: 6rpx;margin-left: 20rpx;}
.vipView{font-size: 24rpx;color: #f15350;margin-left: 20rpx;}
/* 规格 */
.guigeItem{display: inline-block;color: #666;margin-right: 10rpx;vertical-align: top;max-width: 100%;}
.attr_list_t{font-weight: bold;font-size: 28rpx;}

.goods-info{ background: #fff; padding:20rpx 30rpx;}
.goods-info-cont{margin-bottom: 20rpx;}
/* 商品价格 */
.goodsPriceBox{display: flex;align-items: center;line-height: 1;overflow: initial;font-size: 24rpx;}
.goodsPriceCuIco{width: 40rpx;height: 40rpx;background: rgba(241, 83, 80, 0.16);color: #f15350;font-size: 24rpx;font-weight: bold;border-radius: 4rpx;line-height: 40rpx;text-align: center;margin-right: 16rpx;}
.goodsPriceCurrent{color: #f15350;font-weight: bold;}
.goodsPriceNum{font-size: 40rpx;}
.vipPrice{font-size: 24rpx;font-weight: bold;margin-left: 16rpx;align-self: flex-end;}
.vipPricenum{font-size: 32rpx;}
.vipPriceIco{display: inline-block;color: #90561d;background: #d9c184;line-height: 30rpx;font-size: 22rpx;font-weight: bold;padding: 0 10rpx;border-radius: 0 30rpx 30rpx 30rpx;margin-left: 6rpx;vertical-align: top;}
/* 销量信息 */
.goodsSalesInfo{color: #999;font-size: 24rpx;margin-top: 12rpx;display: flex;align-items: center;line-height: 1;}
/* 收藏 */
.goodsLikeBtn{padding: 10rpx 20rpx;margin-left: 20rpx;font-size: 24rpx;color: #999;text-align: center;line-height: 1;align-self: center;}
.goodsLikeBtn .iconfont{ font-size: 40rpx;margin-bottom: 6rpx;}
.goodsLikeBtn.active .iconfont{color: #f15350;}
/* 商品分享 */
.goods-share{background: #f15350;background: var(--DShop-mcolor);color: #fff;margin-right: -30rpx;align-self: center;font-size: 24rpx;padding: 0 10rpx;border-radius: 70rpx 0 0 70rpx;line-height: 68rpx;}
.goods-share-ico{font-size: 36rpx;margin-right: 10rpx;}

/* 商品信息 */
.goodsInfoName{font-weight: bold;line-height: 1.4;}
.goodsInfoDesc{font-size: 28rpx;color: #666;line-height: 1.6;}

/* 商品信息列表 */
.goodsInfoItem{background: #fff;padding: 0 30rpx;line-height: 92rpx;align-items: center;font-size: 28rpx;position: relative;}
.goodsInfoItemTit{color: #666;margin-right: 30rpx;}
.goodsInfoItemMore{font-size: 36rpx;}
.infoListPop{position: fixed;top: 0;left: 0;right: 0;bottom: 0;z-index: 2;}
.infoListPopBg{width: 100%;height: 100%;position: absolute;top: 0;left: 0;background: rgba(0, 0, 0, 0.5);animation: bgShow .1s linear forwards;}
@keyframes bgShow {
    from {opacity: 0;}
    to {opacity: 1;}
}
@keyframes slideup {
    from {transform: translateY(0);}
    to {transform: translateY(100%);}
}
.infoPopContBox{background: #fff;height: 70%;transform: translateY(100%);position: absolute;bottom: 0;left: 0;right: 0;border-radius: 20rpx 20rpx 0 0;display: flex;display: -webkit-flex;flex-direction: column;}
.infoListPopTit{text-align: center;font-size: 32rpx;line-height: 100rpx;}
.infoListPopCont{padding: 0 30rpx;flex: 1;overflow-y: auto}
.infoListPopBtnBox{height: 92rpx;margin-top: 20rpx;}
.infoListPopBtn{width: 100%;border-radius: 0;line-height: 92rpx;font-size: 32rpx;background: #fdae3d;color: #fff;position: absolute;bottom: 0;}
.slidedown{animation: slidedown 0.3s linear forwards;}
.slideup{animation: slideup 0.3s linear forwards;}
@keyframes slidedown {
    from {transform: translateY(0);}
    to {transform: translateY(100%);}
}
@keyframes slideup {
    from {transform: translateY(100%);}
    to {transform: translateY(0);}
}
/* 服务说明 */
.listMsgItem{display: inline-block;margin-right: 16rpx;font-size: 24rpx;}
.listMsgItem .serviceItemIco{font-size: 28rpx;color: #f15350;margin-right: 6rpx;}
