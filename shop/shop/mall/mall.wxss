/* 扩展模块 */
.adver{-webkit-column-gap:0;column-gap:0;} 
.adver image{width:100%;height: 100%;}
.adver .ad-item{float: left;position: relative;width: 100%;-webkit-column-break-inside:avoid;}
.ad-item image{position: absolute;top: 0;left: 0;}

.ad2th{-webkit-column-count:2;column-count:2;}
.ad2th .ad-item{padding-top: 100%;}
.ad3th{-webkit-column-count:3;column-count:3;}
.ad3th .ad-item{padding-top: 120%;}
.ad3th2 .ad-item:first-child{width:46%;padding-top: 60%;}
.ad3th2 .ad-item:nth-child(n+2){width:54%;padding-top: 30%;}
.ad4th{-webkit-column-count:4;column-count:4;}
.ad4th .ad-item{padding-top: 140%;} 
.ad4th2{-webkit-column-count:2;column-count:2;}
.ad4th2 .ad-item{padding-top: 100%;}
.ad4th3 .ad-item:first-child{width:40%;padding-top: 60%;}
.ad4th3 .ad-item:nth-child(2){width:60%;padding-top: 30%;}
.ad4th3 .ad-item:nth-child(3),.ad4th3 .ad-item:nth-child(4){width:30%;padding-top: 30%;}
.ad5th .ad-item:first-child{width: 100%;padding-top:50%;}
.ad5th .ad-item:nth-child(2),.ad5th .ad-item:nth-child(5){width: 50%;padding-top:48%;}
.ad5th .ad-item:nth-child(3),.ad5th .ad-item:nth-child(4){width: 50%;padding-top:52%;}
.ad5th .ad-item:nth-child(4){margin-top: -4%;} 
/* 扩展模块间距 */
.adver{--adver-space:0px;}
.adver.space-mini{--adver-space:2rpx;}
.adver.space-large{--adver-space:16rpx;}
/* .space-tb{margin:20rpx 0;}  */
.adver-box .adver:first-child{margin-top: 0;} 
.adver{-webkit-column-gap:var(--adver-space);column-gap:var(--adver-space);}
.adver.ad3th2 .ad-item:nth-child(n+2){margin-left: var(--adver-space);width:calc(54% - var(--adver-space));padding-top: calc(30% - (var(--adver-space)/2));}
.adver.ad3th2 .ad-item:nth-child(n+3){margin-top: var(--adver-space);}
.adver.ad4th2 .ad-item:nth-child(2n){margin-top: var(--adver-space);} 
.adver.ad4th3 .ad-item:first-child{width: calc(40% - var(--adver-space));} 
.adver.ad4th3 .ad-item:nth-child(n+2){margin-left: var(--adver-space);padding-top: calc(30% - (var(--adver-space)/2));} 
.adver.ad4th3 .ad-item:nth-child(n+3){margin-top: var(--adver-space);width:calc(30% - (var(--adver-space)/2));}
.adver.ad5th .ad-item:nth-child(n+2){width: calc(50% - (var(--adver-space)/2));}
.adver.ad5th .ad-item:nth-child(2),.adver.ad5th .ad-item:nth-child(4){margin-right: var(--adver-space);}
.adver.ad5th .ad-item:nth-child(2),.adver.ad5th .ad-item:nth-child(3),.adver.ad5th .ad-item:nth-child(5){margin-top: var(--adver-space);}
.adver.ad5th .ad-item:nth-child(4){margin-top: calc(var(--adver-space) - 4%);} 

