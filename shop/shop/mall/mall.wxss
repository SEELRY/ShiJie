/* 公告 */
.notice{height: 92rpx;line-height: 92rpx;background: rgba(255, 255, 255, 0.8);padding: 0 30rpx;}
.notice:active{background: #eee;}
.notice-list{height: 92rpx;width: 100%;}
.notice .icon-tongzhi{ transform: scale(1.2);}
.tips-detail{z-index: 1;border-radius: 5px;width: 600rpx;padding-bottom: 30rpx;background: #fff;}
.tips-top{background: #00aeff; letter-spacing: 10rpx; padding: 30rpx;font-weight: bold;font-size: 36rpx;color: #fff;position: relative;border-radius: 5px 5px 0 0;overflow: hidden;} 
.tips-detail .close{width: 36px;height: 36px;color:#fff;border: 1px solid;border-radius: 50%;position: absolute;bottom:-50px;left: 50%;transform: translateX(-50%);}
.tips-detail .close::before,.tips-detail .close::after{content:'';display:block;width: 50%;height: 1px;background: currentColor;position: absolute;top:50%;left: 50%;}
.tips-detail .close::before{transform: translate(-50%,-50%) rotate(45deg);}
.tips-detail .close::after{transform: translate(-50%,-50%) rotate(-45deg);}
.tips-detail-content{max-height:500rpx;overflow: auto;padding: 0 30rpx;}
.tips-detail-item{margin: 20rpx 0;}
.tips-top::before,.tips-top::after{content: '';display: block;width: 100%;padding-top: 100%;background: rgba(255, 255, 255, 0.1);border-radius:50% 40%;position: absolute;bottom:30rpx;}
.tips-top::before{animation: drift 8s linear infinite;left: 10%;} 
.tips-top::after{animation: drift 12s linear infinite;right: 10%;} 
@-webkit-keyframes drift {
  from {-webkit-transform: rotate(0deg);transform: rotate(0deg);}
  to {-webkit-transform: rotate(360deg);transform: rotate(360deg);}
}
@keyframes drift {
  from {-webkit-transform: rotate(0deg);transform: rotate(0deg);}
  to {-webkit-transform: rotate(360deg);transform: rotate(360deg);}
}

