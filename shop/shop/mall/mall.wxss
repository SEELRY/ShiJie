.countdownClock{ width: 40rpx; height: 40rpx; box-shadow:0 0 0 5rpx currentColor; border-radius: 50%; position: relative; overflow: hidden; transform: scale(.8); transform-origin: left center; margin-right: 6rpx;}
.countdownClock view{ width:10rpx; height:10rpx; background: currentColor;position: absolute; left: 50%; top: 50%; margin: -5rpx 0 0 -5rpx; border-radius: 100px;}
.countdownClock:after,.countdownClock:before{ content: ""; background: currentColor; display: block; width:30%; height: 1px; position: absolute; top: 50%; left:50%; -webkit-transform-origin:left center;transform-origin:left center; -webkit-animation:timeAn 12s linear infinite ;animation:timeAn 12s linear infinite ; border-radius: 2px;}
.countdownClock:after{ height: 3px;}
.countdownClock:before{ width: 40%; -webkit-animation-duration:1s;animation-duration:1s;}
@-webkit-keyframes timeAn{
	from{ -webkit-transform: translateY(-50%) rotate(0deg);}
	to{ -webkit-transform: translateY(-50%) rotate(360deg);}
}
@keyframes timeAn{
	from{ transform: translateY(-50%) rotate(0deg);}
	to{ transform: translateY(-50%) rotate(360deg);}
}
