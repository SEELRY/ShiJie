/* 滚动到顶部时候添加此类 */
.topFix {
  position: fixed;
  left: 0;
  top: 0;
  width: 100%;
  z-index: 9;
  background-color: #fff;
}

/*  */
.main {
  background-color: #fff;
  margin: -25rpx 25rpx 0;
  border-radius: 10rpx 10rpx 0 0;
  overflow: hidden;
  box-shadow: 10rpx 0rpx 20rpx -6rpx rgba(0, 0, 0, 0.05),-10rpx 0 20rpx -6rpx rgba(0, 0, 0, 0.05);
}

.menuBox {
  position: relative;
  white-space: nowrap;
  box-shadow: 0 6rpx 10rpx -4rpx rgba(0, 0, 0, 0.1);
  background: #fff;
  padding: 20rpx 0;
  font-size: 28rpx;
}

.menuBox::before, .menuBox::after {
  content: "";
  position: absolute;
  z-index: 6;
  top: 0;
  height: 100%;
}

.menuBox::after {
  right: 0;
  width: 40rpx;
  background-image: linear-gradient(-90deg, rgba(255, 255, 255, 1), rgba(255, 255, 255, 0.35));
}

.menuBox::before {
  left: 0;
  width: 20rpx;
  background-image: linear-gradient(90deg, rgba(255, 255, 255, 1), rgba(255, 255, 255, 0.35));
}

.menu {
  display: inline-block;
  vertical-align: top;
  padding:0 30rpx;
  line-height: 53rpx;
  position: relative;
  border-radius: 100px;
}
.menu:first-child{
  margin-left: 20rpx;
}
.menu:last-child{
  margin-right: 20rpx;
}
.menu:active{
  background: rgba(0, 0, 0, .1);
}
.menu.active{
  background: var(--DShop-mcolor);
  color:#fff;
}
.menu text{
  position: relative;
  z-index: 1;
}

.menu.mainColor {
  position: relative;
  z-index: 5;
  font-size: 32rpx;
  font-weight: 700;
}

.menu.mainColor::before {
  content: "";
  position: absolute;
  left: 0;
  bottom: 25rpx;
  z-index: 3;
  width: 100%;
  height: 20rpx;
  transform: scaleX(0.55);
  background-color: rgba(var(--mColorRgb), 0.24);
}


/*  */

.advenBox {
  margin-top: 10rpx;
  color: #888;
  font-size: 24rpx;
  justify-content: space-around;
}

.sucIcon {
  transform: translate(6rpx, 4rpx) scale(0.55);
}

/*  */

.communitySwiper {
  position: relative;
  margin-top: 20rpx;
  padding-top: 25%;
}

.communitySwiper .img{
  position: absolute;
  z-index: 1;
  top: 0;
  left: 0;
}
.communitySwiper image {
  width: 100%;
  height: 100%;
  border-radius: 100rpx;
}

.communitySwiper .wx-swiper-dots-horizontal {
  display: flex;
  align-items: center;
  margin-bottom: 10rpx;
}

.communitySwiper .wx-swiper-dots-horizontal .wx-swiper-dot {
  width: 10px;
  height: 2px;
  background-color: rgba(255, 255, 255, 0.8);
  border-radius: 0;
}

.communitySwiper .wx-swiper-dots-horizontal .wx-swiper-dot-active {
  width: 8px;
  height: 8px;
  border-radius: 50%;
  background-color: #fff;
  align-self: center;
}

/* list */

.listBox {
  background: #fff;
  padding-bottom: 120rpx;
}

.newestTit {
  font-size: 34rpx;
  text-align: center;
  color: var(--mColor);
  position: relative;
  margin-bottom: 20rpx;
}

.newestTit::before {
  content: "";
  position: absolute;
  width: 120rpx;
  height: 20rpx;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
}

.goodsItem {
  padding:40rpx 30rpx 40rpx 20rpx;
  background-color: #fff;
  border-bottom:1px solid #eee;
  line-height: inherit;border-radius: 0;text-align: inherit;
}

.gallery {
  justify-content: space-between;
  background: transparent;padding-left: 0;padding-right: 0;border-radius: 0;
}

.imgBox {
  position: relative;
  width: 218rpx;
  height: 218rpx;
  border-radius: 6rpx;
  overflow: hidden;
}

.goodName {
  font-size: 36rpx;
  font-weight: bold;
  margin-top: 20rpx;
  line-height: 1.3;
}

.goodDesc {
  color: #666;
  line-height: 36rpx;
  margin-bottom: 14rpx;
}

.countdown {
  /* font-size: 23rpx; */
  color: #f66;
  /* background: rgba(255, 102, 102, .1);
  padding: 0 16rpx;
  display: inline-block;
  line-height: 49rpx;
  border-radius:100rpx; */
}

.GoodssoldOut .listInfo{
  opacity: .6;
}

.rmbIcon {
  font-size: 16rpx;
}

.priBox {
  margin-top: 20rpx;
  align-items: center;
}

.redPri {
  color: rgba(229, 28, 35, 0.99);
  font-size: 40rpx;
  margin-right: 20rpx;
}

.redPri .rmbIcon {
  font-size: 28rpx;
  margin-left: -4rpx;
}

.rpType {
  text-decoration: line-through;
}

.rpType .rmb {
  font-size: 20rpx;
}

.toPart {
  line-height: 59rpx;
  border-radius: 100rpx;
  font-size: 25rpx;
  text-align: center;
  color: #fff;
  padding: 0 30rpx;
  /* background-color: var(--mColor);  */
  background-color: #f66; 
  box-shadow: 0px 3px 3px -1px rgba(0, 0, 0, .1); 
  transition: all 0.3s ease-in-out;
}

.toPart:active {
  opacity: 0.85;
  transform: translateY(2px);
  box-shadow: 0px 1px 1px -1px rgba(0, 0, 0, .08); 
}

.button {
  border-radius: 100rpx;
  box-shadow: 0 8rpx 0 #dd5288, 0 16rpx 20rpx rgba(0, 0, 0, 0.25);
  transition: box-shadow 0.1s ease-in-out;
  font-size: 32rpx;
  line-height: 2.2;
  color: #fff;
}

.button text {
  display: inline-block;
  border-radius: 100rpx;
  padding: 20rpx 40rpx;
  background-color: #ec528d;
  background-image: -webkit-linear-gradient(hsla(338, 90%, 80%, 0.8), hsla(338, 90%, 70%, 0.2));
  box-shadow: inset 0 -1px 1px rgba(255, 255, 255, 0.25);
  line-height: 1;
  text-shadow: 0 -1px 1px rgba(175, 49, 95, 0.7);
  transition: background-color 0.2s ease-in-out, transform 0.1s ease-in-out;
}

.button:active {
  box-shadow: 0 8rpx 0 #c5376d, 0 12rpx 10rpx rgba(0, 0, 0, 0.2);
}

.button:active text {
  transform: translate(0, 6rpx);
  background-color: #ec6a9c;
  text-shadow: 0 -1px 1px rgba(175, 49, 95, 0.9), 0 0 5px rgba(255, 255, 255, 0.8);
}

/* 跨境角标 */
.crossArea{
  position: absolute;
  z-index: 9;
  top: 0;
  right: 0;
  color: #3a2314;
  background-color: #FBEEEA;
  font-size: 20rpx;
  padding:  0 50rpx;
  height: 14px;
  line-height: 14px;
  transform:translate(46rpx,8rpx) rotate(45deg);

}

.footLinks{ padding: 20rpx; display: flex;}
.footLinks view{ height: 150rpx; background: #eee; flex: 1; overflow: hidden;}
.footLinks view:nth-child(n+2){ margin-left: 20rpx;}
.footLinks image{ width: 100%;}
.footLinks view:only-child{height: auto;}

